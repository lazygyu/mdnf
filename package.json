{
  "name": "mdns",
  "version": "1.0.0",
  "description": "mdn search",
  "author": "lazygyu <lazygyu@gmail.com>",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">= 14"
  },
  "bin": {
    "mdns": "./build/main.js"
  },
  "files": [
    "build/"
  ],
  "scripts": {
    "dev": "tsx watch ./src/main.ts",
    "build": "tsup ./src/main.ts"
  },
  "devDependencies": {
    "tsup": "^6.1.3",
    "tsx": "^3.8.0",
    "typescript": "^4.7.4"
  },
  "tsup": {
    "target": "esnext",
    "format": [
      "esm"
    ],
    "outDir": "./build/"
  }
}
