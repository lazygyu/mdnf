{
  "name": "mdnf",
  "version": "1.0.0",
  "description": "mdn finder",
  "author": "lazygyu <lazygyu@gmail.com>",
  "license": "MIT",
  "type": "module",
  "homepage": "https://github.com/lazygyu/mdnf",
  "repository": "lazygyu/mdnf",
  "engines": {
    "node": ">= 14"
  },
  "bin": {
    "mdns": "./build/main.js"
  },
  "files": [
    "build/"
  ],
  "scripts": {
    "dev": "tsx watch ./src/main.ts",
    "build": "tsup ./src/main.ts",
    "start": "node ./build/main.js"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.1",
    "@types/node": "^18.0.5",
    "tsup": "^6.1.3",
    "tsx": "^3.8.0",
    "typescript": "^4.7.4"
  },
  "tsup": {
    "target": "esnext",
    "format": [
      "esm"
    ],
    "outDir": "./build/"
  },
  "dependencies": {
    "arg": "^5.0.2",
    "fzf": "^0.5.1",
    "inquirer": "^9.0.1"
  }
}
